    "migration:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
