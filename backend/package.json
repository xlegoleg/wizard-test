{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express + TypeORM backend for quiz wizard",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/app.ts",
    "start": "node dist/app.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
